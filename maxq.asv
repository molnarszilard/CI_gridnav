function maxQ=maxq(Q,i,j,k,size)
q0=Q(i,j,k);
if i>1 &  % nem az elso sorban vagyok
    q1=Q(i-1,j,k);
else
    q1=-100;
end
if i<size(1) % lehet menni "a falig", nem az 5. sorban vagyok
    q2=Q(i+1,j,k);
else
    q2=-100;
end
if j>1
    q3=Q(i,j-1,k);
else
    q3=-100;
end
if j<size(2)
    q4=Q(i,j+1,k);
else
    q4=-100;
end
maxQ=max([q0,q1,q2,q3,q4]);
end